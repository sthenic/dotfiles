{
    "workbench.colorTheme": "Gruvbox Dark (Medium)",
    "workbench.iconTheme": null,
    "window.menuBarVisibility": "hidden",
    "editor.fontFamily": "'DejaVu Sans Mono', 'Inconsolata', 'Consolas', 'monospace', monospace",
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.fontSize": 14,
    "editor.fontWeight": "normal",
    "editor.insertSpaces": true,
    "editor.matchBrackets": true,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.occurrencesHighlight": false,
    "editor.rulers": [
        80
    ],
    "editor.scrollBeyondLastLine": true,
    "editor.tabSize": 4,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.eol": "\n",
    "editor.renderWhitespace": "none",
    "editor.renderIndentGuides": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "latex-workshop.latex.autoBuild.onSave.enabled": false,
    "[tex]": {
        "editor.wordSeparators": "`~!#$%^&*()-=+[{]}\\|;:'\",.<>/?",
        "editor.quickSuggestions": {
            "other": false,
            "comments": false,
            "strings": false
        },
        "editor.tabSize": 2
    },
    "[latex]": {
        "editor.wordSeparators": "`~!#$%^&*()-=+[{]}\\|;:'\",.<>/?",
        "editor.quickSuggestions": {
            "other": false,
            "comments": false,
            "strings": false
        },
        "editor.tabSize": 2
    },
    "workbench.sideBar.location": "left",
    "editor.wordWrapColumn": 80,
    "[plaintext]": {
        "editor.wordWrap": "wordWrapColumn"
    },
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn"
    },
    "workbench.startupEditor": "newUntitledFile",
    "python.linting.pylintEnabled": false,
    "restructuredtext.linter.run": "off",
    "python.pythonPath": "python3",
    "search.location": "panel",
    "[verilog]": {
        "editor.tabSize": 3
    },
    "workbench.statusBar.feedback.visible": false,
    "editor.detectIndentation": false,
    "rewrap.wrappingColumn": 80,
    "window.zoomLevel": 0,
    "latex-workshop.message.update.show": false,
    "diffEditor.renderSideBySide": false,
    "terminal.integrated.shell.windows": "C:\\cygwin\\bin\\bash.exe",
    "terminal.integrated.shellArgs.windows": [
        "/bin/xhere",
        "/bin/bash"
    ],
    "[nim]": {
        "editor.tabSize": 3
    },
    "editor.smoothScrolling": false,
    "editor.autoIndent": false,
    "materialTheme.fixIconsRunning": false,
    "workbench.colorCustomizations": {
        "[Gruvbox Dark (Medium)]": {
            "statusBar.background": "#333333",
            "list.focusBackground": "#333333",
            "input.border": "#555555",
            "focusBorder": "#555555",
            "list.highlightForeground": "#b8bb26",
            "selection.background": "#b8bb265f",
            "list.activeSelectionBackground": "#444444",
            "foreground": "#fbf1c7dd",
            "gitDecoration.untrackedResourceForeground": "#d3869b",
            "gitDecoration.ignoredResourceForeground": "#fbf1c766",
            "gitDecoration.modifiedResourceForeground": "#b8bb26",
            "tab.activeForeground": "#fbf1c7dd",
            "tab.inactiveForeground": "#fbf1c766",
            "activityBarBadge.background": "#83851b",
            "extensionButton.prominentBackground": "#83851b",
            "terminal.ansiGreen": "#b8bb26",
            "terminal.ansiBrightGreen": "#b8bb26",
            "terminal.ansiBlue": "#5092dc",
            "terminal.ansiBrightBlue": "#5092dc",
            "terminal.ansiRed": "#fb4934",
            "terminal.ansiBrightRed": "#fb4934",
            "terminal.ansiYellow": "#fabd2f",
            "terminal.ansiBrightYellow": "#fabd2f",
            "input.placeholderForeground": "#fbf1c766",
            "pickerGroup.foreground": "#b8bb26",
            "list.warningForeground": "#fabd2f",
            "list.errorForeground": "#fb4934",
            "list.inactiveSelectionBackground": "#444444",
            "sideBarSectionHeader.background": "#333333",
            "activityBar.background": "#333333"
        }
    }
}
