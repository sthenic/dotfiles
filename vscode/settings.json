{
    "workbench.iconTheme": null,
    "window.menuBarVisibility": "default",
    "editor.fontFamily": "'JetBrains Mono Semi Light', 'Iosevka Extended', 'Iosevka', 'Input Mono Narrow', 'Victor Mono Semibold', 'Cascadia Code', 'DejaVu Sans Mono', 'Inconsolata', 'Consolas', 'monospace', monospace",
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.fontSize": 14,
    "editor.fontWeight": "normal",
    "editor.insertSpaces": true,
    "editor.matchBrackets": "always",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.occurrencesHighlight": false,
    "editor.fontLigatures": true,
    "editor.rulers": [
        100
    ],
    "editor.scrollBeyondLastLine": true,
    "editor.tabSize": 4,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.eol": "\n",
    "editor.renderWhitespace": "none",
    "editor.renderIndentGuides": true,
    "telemetry.telemetryLevel": "off",
    "[tex]": {
        "editor.wordSeparators": "`~!#$%^&*()-=+[{]}\\|;:'\",.<>/?@",
        "editor.quickSuggestions": {
            "other": false,
            "comments": false,
            "strings": false
        },
        "editor.tabSize": 2
    },
    "[latex]": {
        "editor.wordSeparators": "`~!#$%^&*()-=+[{]}\\|;:'\",.<>/?@",
        "editor.quickSuggestions": {
            "other": false,
            "comments": false,
            "strings": false
        },
        "editor.tabSize": 2
    },
    "workbench.sideBar.location": "left",
    "editor.wordWrapColumn": 80,
    "[plaintext]": {
        "editor.wordWrap": "wordWrapColumn"
    },
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn"
    },
    "workbench.startupEditor": "newUntitledFile",
    "python.linting.pylintEnabled": false,
    "restructuredtext.linter.run": "off",
    "python.pythonPath": "python3",
    "[verilog]": {
        "editor.tabSize": 4,
        "editor.fontLigatures": false
    },
    "workbench.statusBar.feedback.visible": false,
    "editor.detectIndentation": false,
    "rewrap.wrappingColumn": 80,
    "latex-workshop.message.update.show": false,
    "diffEditor.renderSideBySide": false,
    "[nimble]": {
        "editor.tabSize": 3,
    },
    "[nim]": {
        "editor.tabSize": 3,
        "editor.snippetSuggestions": "none",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": false
        },
        "editor.wordBasedSuggestions": false,
        "editor.parameterHints.enabled": false,
        "editor.suggestOnTriggerCharacters": false
    },
    "svn.autorefresh": false,
    "editor.smoothScrolling": false,
    "editor.autoIndent": "brackets",
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\bash.exe",
    "terminal.integrated.shellArgs.windows": ["-i"],
    "terminal.integrated.fontFamily": "'Iosevka Extended', 'Iosevka', 'Input Mono Narrow', 'Victor Mono Semibold', 'Cascadia Code', 'DejaVu Sans Mono', 'Inconsolata', 'Consolas', 'monospace', monospace",
    "terminal.integrated.fontSize": 14,
