            "activityBarBadge.foreground": "#202020"
        },
        "[Grueling Hare Dark]": {
            "editor.findMatchBackground": "#744418",
            "editor.findMatchHighlightBackground": "#744418"
        }
    },
    "editor.tokenColorCustomizations": {
        "[Horizon]": {
            "textMateRules": [
                {
                    "name": "Comments",
                    "scope": "comment",
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        },
        "[Nord]": {
            "comments": "#6d7a96"
        },
        "[nil]": {
            "comments": "#666666",
            "strings": "#ADB4B5"
        }
    },
    "window.titleBarStyle": "custom",
    "workbench.settings.editor": "json",
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.cwd": "${fileDirname}",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "update.mode": "manual",
    "extensions.autoUpdate": false,
    "workbench.editor.enablePreview": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.hovers.enabled": false,
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true
    },
    "gitlens.codeLens.enabled": false,
    "restructuredtext.preview.scrollPreviewWithEditor": false,
    "restructuredtext.preview.scrollEditorWithPreview": false,
    "editor.parameterHints.enabled": false,
    "latex-workshop.latex.autoBuild.run": "never",
    "files.exclude": {
        "**/*.db": true,
        "**/*.png": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true
    },
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[html]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[yaml]": {
        "editor.tabSize": 2
    },
    "svn.enableProposedApi": "none",
    "prettier.proseWrap": "always",
    "prettier.trailingComma": "all",
    "python.formatting.provider": "black",
    "todo-tree.highlights.enabled": false,
    "editor.quickSuggestionsDelay": 500,
    "latex-workshop.view.pdf.viewer": "tab",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.limit.value": 4,
    "todo-tree.tree.showScanModeButton": false,
    "chronicler.ffmpeg-binary": "/usr/bin/ffmpeg",
    "chronicler.debug": true,
    "chronicler.dest-folder": "~/Recordings",
    "chronicler.recording-defaults": {
        "flags": null,
        "fps": 10,
        "countdown": 3
    },
    "workbench.colorTheme": "Minimal Fedu Ext",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.enableTelemetry": false,
    "extensions.ignoreRecommendations": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "breadcrumbs.enabled": false
}
